  case $ac_file in
    *.$ac_ext | *.xcoff | *.tds | *.d | *.pdb | *.xSYM | *.bb | *.bbg | *.map | *.inf | *.dSYM | *.o | *.obj )
	;;
    [ab].out )
	# We found the default executable, but exeext='' is most
	# certainly right.
	break;;
    *.* )
	if test "${ac_cv_build_exeext+set}" = set && test "$ac_cv_build_exeext" != no;
	then :; else
	   ac_cv_build_exeext=`expr "$ac_file" : '[^.]*\(\..*\)'`
